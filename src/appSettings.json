{
  "application": {
    "name": "SmartBackup"
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "%PROGRAMDATA%\\geheb\\SmartBackup\\logs\\SmartBackup.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31
        }
      },
      { "Name": "Console" }
    ]
  }
}
